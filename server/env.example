# Server Configuration
NODE_ENV=development
PORT=8080

# Ethereum Configuration
INFURA_PROJECT_ID=your_infura_project_id_here
INFURA_PROJECT_SECRET=your_infura_project_secret_here
ETHEREUM_NETWORK=sepolia

# Security Configuration
JWT_SECRET=your_jwt_secret_here_min_32_characters
CORS_ORIGIN=http://localhost:3000

# Database Configuration (if needed in future)
# DATABASE_URL=mongodb://localhost:27017/mpc-wallet

# Logging Configuration
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# MPC Configuration
MPC_SESSION_TIMEOUT_MS=1800000
MPC_CLEANUP_INTERVAL_MS=300000

# Development Settings
ENABLE_DEBUG_LOGS=true 